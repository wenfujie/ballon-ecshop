{
    "name": "server",
    "version": "1.0.0",
    "description": "webServer on node by koa2",
    "main": "app.js",
    "scripts": {
        "dev": "cross-env LOCAL_NODE_ENV=development nodemon --inspect ./bin/app.js ",
        "test": "cross-env LOCAL_NODE_ENV=test nodemon --inspect ./bin/app.js ",
        "test2": "cross-env LOCAL_NODE_ENV=test2 nodemon --inspect ./bin/app.js ",
