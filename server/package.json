        "koa-bodyparser": "^4.2.0",
        "koa-cors": "0.0.16",
        "koa-router": "^7.4.0",
        "koa-session": "^5.8.2",
        "koa-static": "^4.0.3",
        "koa-views": "^6.1.4",
        "log4js": "^4.4.0",
        "moment": "^2.22.2",
        "nodemon": "^1.8.1",
        "pm2": "^2.10.4",
